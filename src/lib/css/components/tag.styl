.rt-tabs--tag-mode .rt-tabs-navigation__item .rt-tabs-navigation__item-name
  font-size 14px
  background-color main-color003
  border-radius 25px
  margin-top 10px
  margin-bottom 10px
  margin-left 10px
  margin-right 10px
  padding-top 6px
  padding-bottom 4px
  padding-left 15px
  padding-right 15px
  cursor pointer
  transition-timing-function ease-in-out
  transition-duration .4s
  transition-property background-color, color



  &:hover,
  &--is-active
    //.{newAppName}-dark-theme &
    //  color b2c-white
    //  background-color b2c-purple

    color b2c-white
    background-color b2c-purple
