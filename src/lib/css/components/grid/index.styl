  -ms-flex-wrap wrap
  &-scroll
    flex-flow nowrap
    overflow auto
  &-nowrap
    flex-wrap: nowrap


  &--tablet-scroll
    @media (max-width: tablet-upper-limit)
      flex-flow nowrap
      overflow auto
      margin-left -1 * horizontal-space * .5
      margin-right -1 * horizontal-space * .5
      padding-bottom vertical-space

  &--tablet-only-scroll
    @media (max-width: tablet-upper-limit) and (min-width: tablet-lower-limit)
