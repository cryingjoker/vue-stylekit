.{newAppName}-col
  flex 0 0 1
  width 100%
  padding-left (horizontal-space * .5)
  padding-right (horizontal-space * .5)
  position relative

for devideStep in steps
  media = 0
  devicePrefix = ""
  needMedia = false

  if (devideStep[1] < 12)
    media = devideStep[3]
    devicePrefix = "-" + devideStep[2]

    if (media)
      needMedia = true

  for $i in 1 .. devideStep[1]
    className = newAppName + "-col" + devicePrefix + "-" + $i

    .{className}
      +cache("mobile-column")
        width 100%
        padding-left (horizontal-space * .5)
        padding-right (horizontal-space * .5)
        position relative

  gridRender(devideStep[1], devicePrefix, devideStep[0], needMedia, media)

  for $i in 0 .. 20
    step = $i * 5
    spaceRender(devicePrefix, needMedia, step, media)

.relative
  position relative

.row
  display flex
  flex-wrap wrap
  min-width 100%
  box-sizing border-box
  margin-left -1 * horizontal-space * .5
  margin-right -1 * horizontal-space * .5
