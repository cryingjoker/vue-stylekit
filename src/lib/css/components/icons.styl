oldIcon()
  border: none
  fill: transparent
  stroke: #fff
  padding: 0
  *
    fill: inherit
    stroke: inherit
  g
    opacity: 1
  [pid="0"]
    stroke-width: 0

deprecatedIcon()
  border: none
  fill: transparent
  stroke: #fff
  padding: 10px
  *
    fill: inherit
    stroke: inherit
  g
    opacity: 1

.rt-icon
  opacity 1
  position: relative
  visibility visible
  transition-timing-function ease-in-out
  transition-duration .4s
  transition-property opacity,visibility

  &__wrapper
    display: inline-block

  &.wc-icon
    border: none
    border-radius: 50%
    text-align: center
    vertical-align: middle

  .caption.top
    background: inherit
    border: 2px solid #fff
    border-radius: 10px
    color: white
    font-size: 12px;
    line-height: 13px
    min-width: 21px
    height: 21px
    padding: 2px 4px
    position: absolute
    top: -8px
    right: -2px

  .icon-speed__spinned-arrow
    margin: auto
    position: absolute
    width: 100%
    height: 100%
    top: 0
    left: 0
    right: 0
    bottom: 0
    svg
      margin: auto
      position: absolute
      width: 100%
      height: 100%
      top: 0
      left: 0
      right: 0
      bottom: 0
    circle
      stroke-width: 1.4px
      stroke: #ffffff
      transition: stroke-dashoffset 2s ease
    path
      transition: transform 2s ease

  &_w
    &55
      width: 55px
      height: 55px
    &56
      width: 56px
      height: 56px
    &80
      width: 80px
      height: 80px
